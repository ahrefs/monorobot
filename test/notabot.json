{
    "main_branch_name": "develop",
    "status_rules": {
        "allowed_pipelines": [
            "default",
            "buildkite/notabot-test"
        ],
        "rules": [
            {
                "on": ["failure"],
                "when": {
                    "match": {
                        "field": "description",
                        "re": "^\\(Build #[0-9]+ canceled by .+\\|Failed (exit status 255)\\)$"
                    }
                },
                "policy": "ignore"
            },
            { "on": ["pending"], "policy": "ignore"},
            { "on": ["failure", "error"], "policy": "allow"},
            { "on": ["success"], "policy": "allow_once"}
        ]
    },
    "prefix_rules": {
        "default_channel": "default",
        "rules": [
            {
                "allow": [
                    "backend/api"
                ],
                "channel": "aa"
            },
            {
                "allow": [
                    "backend/api/longest"
                ],
                "channel": "longest-aa"
            },
            {
                "allow": [
                    "backend/megaindex",
                    "backend/ahrefskit"
                ],
                "channel": "backend"
            },
            {
                "channel": "all-push-events"
            }
        ]
    },
    "label_rules": {
        "default_channel": "default",
        "rules": [
            {
                "allow": [
                    "backend"
                ],
                "channel": "backend"
            },
            {
                "allow": [
                    "aa"
                ],
                "channel": "aa-git"
            },
            {
                "allow": [
                    "siren"
                ],
                "channel": "siren"
            },
            {
                "ignore": [
                    "backend",
                    "aa",
                    "siren"
                ],
                "channel": "frontend-bot"
            }
        ]
    }
}
