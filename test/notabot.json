{
    "main_branch_name": "develop",
    "status_rules": {
        "allowed_pipelines": [
            "default",
            "buildkite/pipeline2"
        ],
        "rules": {
            "pending": false,
            "failure": true,
            "error": true,
            "cancelled": "^\\(Build #[0-9]+ canceled by .+\\|Failed (exit status 255)\\)$",
            "success": "once"
        }
    },
    "prefix_rules": {
        "default_channel": "default",
        "rules": [
            {
                "allow": [
                    "backend/a1"
                ],
                "channel": "a1"
            },
            {
                "allow": [
                    "backend/a1/longest"
                ],
                "channel": "longest-a1"
            },
            {
                "allow": [
                    "backend/a5",
                    "backend/a4"
                ],
                "channel": "backend"
            },
            {
                "channel": "all-push-events"
            }
        ]
    },
    "label_rules": {
        "default_channel": "default",
        "rules": [
            {
                "allow": [
                    "backend"
                ],
                "channel": "backend"
            },
            {
                "allow": [
                    "a1"
                ],
                "channel": "a1-bot"
            },
            {
                "allow": [
                    "a3"
                ],
                "channel": "a3"
            },
            {
                "ignore": [
                    "backend",
                    "a1",
                    "a3"
                ],
                "channel": "frontend-bot"
            }
        ]
    }
}
