{
    "main_branch_name": "develop",
    "status_rules": {
        "allowed_pipelines": [
            "default",
            "buildkite/notabot-test"
        ],
        "rules": {
            "pending": false,
            "failure": true,
            "error": true,
            "cancelled": "^\\(Build #[0-9]+ canceled by .+\\|Failed (exit status 255)\\)$",
            "success": "once"
        }
    },
    "prefix_rules": {
        "default_channel": "default",
        "rules": [
            {
                "allow": [
                    "backend/api"
                ],
                "ignore": [],
                "channel": "aa"
            },
            {
                "allow": [
                    "backend/api/longest"
                ],
                "ignore": [],
                "channel": "longest-aa"
            },
            {
                "allow": [
                    "backend/megaindex",
                    "backend/ahrefskit"
                ],
                "ignore": [],
                "channel": "backend"
            },
            {
                "allow": [],
                "ignore": [],
                "channel": "all-push-events"
            }
        ]
    },
    "label_rules": {
        "default_channel": "default",
        "rules": [
            {
                "allow": [
                    "backend"
                ],
                "ignore": [],
                "channel": "backend"
            },
            {
                "allow": [
                    "aa"
                ],
                "ignore": [],
                "channel": "aa-git"
            },
            {
                "allow": [
                    "siren"
                ],
                "ignore": [],
                "channel": "siren"
            },
            {
                "allow": [],
                "ignore": [
                    "backend",
                    "aa",
                    "siren"
                ],
                "channel": "frontend-bot"
            }
        ]
    }
}
